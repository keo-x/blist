{
  "name": "blist",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "build": "concurrently \"npm run build:nest\" \"npm run build:remix\" -n \"NEST,REMIX\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently \"npm run start:dev:nest\" \"npm run start:dev:remix\" -n \"NEST,REMIX\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "pnpm typeorm-ts-node-commonjs",
    "migration:generate": "pnpm run typeorm -d src/config/migration.config.ts migration:generate src/database/migrations/$npm_config_name",
    "migration:create": "pnpm run typeorm migration:create src/database/migrations/$npm_config_name",
    "migration:run": "pnpm run typeorm -d src/config/migration.config.ts migration:run",
    "build:nest": "rimraf dist; nest build -p tsconfig.nest.json",
    "build:remix": "rimraf build; remix build",
    "start:dev:nest": "rimraf dist; nest start --watch -p tsconfig.nest.json",
    "start:dev:remix": "rimraf build; remix watch"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "latest",
    "@nestjs/typeorm": "^9.0.1",
    "@remix-run/express": "latest",
    "@remix-run/node": "latest",
    "@remix-run/react": "latest",
    "@remix-run/serve": "latest",
    "apollo-server-express": "^3.11.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "lint-staged": "^13.1.1",
    "nest-remix": "^0.0.1-alpha6",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-magic-login": "^1.2.2",
    "pg": "^8.9.0",
    "rambda": "^7.4.0",
    "react": "latest",
    "react-dom": "latest",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@remix-run/dev": "latest",
    "@remix-run/eslint-config": "latest",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/react": "latest",
    "@types/react-dom": "^18.0.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@typescript-eslint/utils": "^5.50.0",
    "concurrently": "latest",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "28.1.1",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
